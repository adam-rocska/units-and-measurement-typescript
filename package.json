    "math",
    "dimension",
    "scaling",
    "measurement-units",
    "unit-operations",
    "unit-management",
    "arithmetic-operations",
    "comparison",
    "tree-shaking",
    "modular",
    "type predicates",
    "value extraction",
    "boilerplate-free",
    "measurements library"
  ],
  "author": {
    "name": "Ádám László Rocska",
    "url": "https://adam-rocska.github.io"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adam-rocska/units-and-measurement-typescript.git"
  },
  "bugs": {
